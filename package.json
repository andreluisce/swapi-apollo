{
  "name": "swapi-apollo",
  "version": "0.0.1",
  "description": "An Apollo server wrapping SWAPI",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/**/*",
    "start": "yarn build && node ./dist/src/index.js",
    "start:publish": "node ./dist/src/index.js",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "dev:debug": "nodemon --exec node --inspect-brk --require ts-node/register ./src/index.ts"
  },
  "keywords": [
    "Apollo-Stack",
    "GraphQL"
  ],
  "author": "Nick Nance",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/schema": "^9.0.9",
    "apollo-server": "^3.11.1",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "body-parser": "^1.20.1",
    "config": "^3.3.8",
    "dataloader": "^2.1.0",
    "express": "^4.17.1",
    "fs-extra": "^10.1.0",
    "graphql": "15.3.0",
    "netlify-lambda": "^2.0.15",
    "request": "^2.88.2",
    "ts-node": "^10.9.1",
    "typescript-bundle": "^1.0.18"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.14",
    "@types/graphql": "^14.5.0",
    "apollo-server-types": "^3.7.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
